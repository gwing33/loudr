    <div id='main' class='container'>
      <h1><small>Documentation</small></h1>

      <ul class="nav nav-tabs">
        <li class="active">
          <a href="#">RESTful API</a>
        </li>
        <li><a href="#">JavaScript Plugin</a></li>
      </ul>

      <h4>Users</h4>

<pre>{
  _id: @._id,
  email: @.email,
  name: {
    first: @name.first,
    last: @name.last,
    full: @.name.full
  },
  loginAttempts: @.loginAttempts,
  locked: @.locked
}</pre>

      <ul class='unstyled'>
        <li>GET /user/:email</li>
        <li>POST /user</li>
        <li>PUT /user/:id</li>
        <li>DELETE /user/:id</li>
      </ul>
      

      <h4>Projects</h4>
<pre>{
  _id: @_id,
  name: @name,
  api: {
    key: @api.key
  },
  disabled: @disabled,
  users: @users,
  info: @info
}</pre>

      <ul class='unstyled'>
        <li>GET /project</li>
        <li>GET /project/:id</li>
        <li>POST /project</li>
        <li>PUT /project/:id</li>
        <li>DELETE /project/:id</li>
        <li>DELETE /project/confirm/:id</li>
        <li>DELETE /project/disable/:id</li>
        <li>GET /project/enable/:id</li>
      </ul>


      <h4>Fans</h4>
<pre>{
  _id: @_id,
  name: @name.full,
  email: @email,
  social: @social,
  api: {
    key: @api.key
  },
  notifications: @notifications,
  groups: @groups,
  info: @info
}</pre>

      <ul class='unstyled'>
        <li>GET /app/:key/fans/</li>
        <li>GET /app/:key/fan/:email</li>
        <li>POST /app/:key/fan</li>
        <li>PUT /app/:key/fan/:email</li>
        <li>DELETE /app/:key/fan/:email</li>
      </ul>

      <h4>Fan Notifications</h4>
<pre>{
  _id: @_id,
  html: @html,
  text: @text,
  url: @url,
  kind: @kind,
  format: @format,
  interacted: @interacted,
  dissmissed: @dissmissed,
  created: @created,
  updated: @updated
}</pre>

      <ul class='unstyled'>
        <li>GET /app/:key/fan/:email/notes</li>
        <li>GET /app/:key/fan/:email/note/:id</li>
        <li>post /app/:key/fan/:email/note</li>
        <li>put /app/:key/fan/:email/note/:id</li>
        <li>del /app/:key/fan/:email/note/:id</li>
      </ul>
